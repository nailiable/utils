{
  "name": "@naiable/util",
  "version": "0.0.1",
  "packageManager": "pnpm@8.8.0",
  "description": "A utility library by naily",
  "author": "Naily <gczgroup@qq.com> (https://github.com/nailiable)",
  "license": "MIT",
  "exports": {
    ".": {
